{
  "name": "react-gas",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "glogin": "clasp login",
    "glogout": "clasp logout",
    "gcreate": "clasp create --title 'React GAS Server' --rootDir ./apps-script",
    "gclone": "clasp clone <SCRIPT ID> --rootDir ./apps-script",
    "gpull": "clasp pull",
    "gpush": "clasp push",
    "gstart": "clasp push --watch",
    "start": "parcel public/index.html --dist-dir ./apps-script",
    "build": "parcel build public/index.html --dist-dir ./apps-script",
    "deploy": "current_deployment=$(clasp deployments | tail -n1 | cut -d' ' -f2) && clasp deploy --deploymentId ${current_deployment} && clasp pull"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@google/clasp": "^2.4.1",
    "parcel": "^2.7.0",
    "process": "^0.11.10"
  },
  "dependencies": {
    "framer-motion": "^7.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "styled-components": "^5.3.5"
  }
}